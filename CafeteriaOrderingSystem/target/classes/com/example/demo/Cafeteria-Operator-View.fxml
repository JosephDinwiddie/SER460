<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.CafeteriaOperatorViewUI">
   <children>
      <Label layoutX="153.0" layoutY="14.0" text="Cafeteria Operator" textAlignment="CENTER">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="25.0" layoutY="351.0" mnemonicParsing="false" onAction="#switchToMainUI" text="Back" />
       <Button fx:id="viewOrdersButton" layoutX="100.0" layoutY="100.0" mnemonicParsing="false" onAction="#viewOrders" text="View Orders" />
      <Button fx:id="updateOrderStatusButton" layoutX="100.0" layoutY="200.0" mnemonicParsing="false" onAction="#updateOrderStatus" text="Update Order Status" />
   </children>
</AnchorPane>
