<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.UnderConstructionController">
   <children>
      <Label layoutX="150.0" layoutY="100.0" text="This Area Is Under Construction" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <ImageView fx:id="constructionImage" layoutX="200.0" layoutY="150.0" fitWidth="200.0" fitHeight="200.0" />
      <Button fx:id="backButton" layoutX="25.0" layoutY="350.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
</AnchorPane>